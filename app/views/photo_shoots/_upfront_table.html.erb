<table class="bordered-table" data-calendly-target="table">
        <thead>
          <tr>
            <th>Customer Name</th>
            <th>Phone Number</th>
            <th>Email</th>
            <th>Type of Shoot</th>
            <th>Amount Paid</th>
            <th>Actions</th>
          </tr>
        </thead>
        <tbody>
          <% photoshoots.each do |photoshoot| %>
            <tr>
              <td><%= photoshoot.appointment.name %></td>
              <td><% phone_number = photoshoot.appointment.questions.find { |q| q.question == 'Phone number' } %>
                    <% if phone_number %>
                      <%= phone_number.answer %></td>
                    <% end %>
              <td><%= photoshoot.appointment.email %></td>
              <td><%= photoshoot.type_of_shoot %></td>
              <td><%= photoshoot.payment_total %></td>
              <td>
                <p><%= link_to 'Update Details', "#" %></p>
              </td>
            </tr>
          <% end %>
        </tbody>
      </table>
